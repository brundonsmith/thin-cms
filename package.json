{
  "name": "thin-cms",
  "version": "0.2.1",
  "description": "A lean, mean, headless CMS built in Angular2 that will let you manage any existing Mongoose schemas and then some without designing your project around it.",
  "main": "index.js",
  "scripts": {
    "dev": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "npm run typings ; npm run tsc",
    "start": "node index.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings install"
  },
  "keywords": [
    "cms",
    "headless",
    "angular2",
    "mongoose"
  ],
  "author": "Brandon Smith",
  "license": "ISC",
  "repository": "git@github.com:brundonsmith/thin-cms.git",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "angular2-in-memory-web-api": "0.0.14",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.6",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.0",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "mongoose": "^4.5.8",
    "reflect-metadata": "^0.1.3",
    "require-dir": "^0.3.0",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0"
  }
}
